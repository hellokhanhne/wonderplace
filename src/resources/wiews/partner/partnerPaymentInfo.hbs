   <body style="overflow-x : hidden;margin: 0;" >
      <div class="ontop" id="ontopbro" onclick="ontop()">
         <i class="fas fa-arrow-alt-circle-up"></i>
      </div>
      <div id="loading">
         <img src="/image/load.gif" alt="">
      </div>
      <input type="hidden" id="about">
   {{>partnerHeader}}
      <div class="container">
         <h3 class="mt-5"><b>Payment infomation</b></h3>
         <div class="row mt-4">
            <div class="col-sm-6">
               <p>Bank *</p>
               <select class="form-control " id="bank" aria-label=".form-select-lg example">
                  <option selected>Choose bank</option>
               </select>
            </div>
            <div class="col-sm-6">
               <p>Bank branch</p>
               <select class="form-control " id="bankBranch" aria-label=".form-select-lg example">
                  <option selected>Choose bank branch</option>
                  
               </select>
            </div>
         </div>
         
         <div class="row mt-4">
            <div class="col-sm-6">
               <p>Account number *</p>
               <input type="number" value="97072022432281" placeholder="Enter account number" name="" id=""  class="form-control">
            </div>
            <div class="col-sm-6">
               <p>Account name *</p>
               <input type="text" value="NGUYEN NGOC KHANH" placeholder="Enter account name" name="" id=""  class="form-control">
            </div>
         </div>

         <h3 class="mt-4"><b>Bill infomation</b></h3>

          <div class="row mt-4">
            <div class="col-sm-6">
               <p>Company name *</p>
               <input type="text" value="Hoang Ha limited company" placeholder="Enter company name" name="" id=""  class="form-control">
            </div>
            <div class="col-sm-6">
               <p>Company representative *</p>
               <input type="text" value="Nguyen Ngoc Khanh" placeholder="Enter company representative" name="" id=""  class="form-control">
            </div>
         </div>
           <div class="row mt-4">
               <div class="col-12">
                   <p>Address</p>
                   <input type="text" value="470 Tran Dai Nghia - Ngu Hanh Son - Da Nang" placeholder="Enter tax code" name="" id=""  class="form-control">
               </div>
           </div>
         <div class="row mt-4">
            <div class="col-sm-6">
               <p>Tax code *</p>
               <input type="text" value="30005591" placeholder="Enter tax code" name="" id=""  class="form-control">
            </div>
            <div class="col-sm-6">
               <p>Email*</p>
               <input type="email" value="Khanhjj70@gmail.com" placeholder="Enter email" name="" id=""  class="form-control">
            </div>
         </div>

         <div class="row ml-3 mt-4">
         <button class="btn btn-primary mr-4" >Save</button>
         <button class="btn btn-secondary">Close</button>
      </div>
      </div>
      
      </div>
     
     <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
      <script src="/js/app.js"></script>
      <script src="/js/app2.js"></script>
      <script>
          $.get("/partnership/get-banks", function(data, status){
              for(let i=0; i<data.length;i++) 
              {
                  document.getElementById('bank').innerHTML += ` <option value='${data[i].bankId}'>${data[i].en_name}</option>`
                   document.getElementById('bankBranch').innerHTML += ` <option value='${data[i].bankId}'>${data[i].en_name}</option>`
              }
              $("#bank").val("970416").change();
              $("#bankBranch").val("970416").change();
        });
      </script>
